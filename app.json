{
  "expo": {
    "name": "CaliWeather",
    "slug": "CaliWeather",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/images/splashScreen.png",
    "scheme": "caliweather",
    "userInterfaceStyle": "automatic",
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/309afd1b-9288-499a-826c-39fc870b78d8"
    },
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kwickslow30.CaliWeather"
    },
    "android": {
      "adaptiveIcon": {
      "foregroundImage": "./assets/images/splashScreen.png",
      "backgroundColor": "#6A3DE8"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.kwickslow30.CaliWeather",
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splashScreen.png",
          "resizeMode": "contain",
          "backgroundColor": "#6A3DE8"
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "309afd1b-9288-499a-826c-39fc870b78d8"
      }
    }
  }
}
